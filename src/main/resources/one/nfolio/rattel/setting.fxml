<AnchorPane fx:id="superRoot" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: rgba(0, 0, 0, 0.7);" stylesheets="@style.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="one.nfolio.controller.SettingController">
   <children>
      <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="ゲーム設定" AnchorPane.topAnchor="0.0">
         <font>
            <Font size="70.0" />
         </font>
      </Text>
      <Separator fx:id="separator" prefWidth="600.0" AnchorPane.topAnchor="75.0" />
      <VBox prefHeight="400.0" prefWidth="600.0" AnchorPane.topAnchor="90.0">
         <children>
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="BGM：ボリューム" />
            <Slider fx:id="bgmVolume" blockIncrement="0.1" max="1.0" />
            <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="マイク：ボリューム" />
            <Slider fx:id="micVolume" blockIncrement="0.1" max="1.0" />
            <CheckBox mnemonicParsing="false" prefHeight="29.0" prefWidth="146.0" text="ミュート" textFill="WHITE" />
         </children>
      </VBox>
   </children>
</AnchorPane>
